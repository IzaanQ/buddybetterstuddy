{
  "manifest_version": 2,
  "name": "buddybetterstuddy",
  "js": "app.js",
  "description": "studying accountability monitor",
  "version": "1.0",
  "icons": {
    "128": "/images/buddy better studdy final.png"
  },

  "content_scripts": [
  {
    "matches": ["https://*/*"],
    "js": ["app.js"]
  }
],

  "browser_action": {
    "default_icon": "/images/buddy better studdy final.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
